    "Function block":{
        "prefix": "fb",
        "description":"Function block template",
        "body": [
            "\/\/Compiler Option --------------------------------------------",
            "{S7ServerActive              := 'n'  }",
            "{SetOKFlag                   := 'n'  }",
            "{MonitorArrayLimits          := 'n'  }",
            "{OverwriteBlocks             := 'y'  }",
            "{CreateObjectCode            := 'y'  }",
            "{OptimizeObjectCode          := 'y'  }",
            "{CreateDebugInfo             := 'y'  }",
            "{GenerateReferenceData       := 'y'  }",
            "{SetMaximumStringLength      := '254'}",
            "\/\/------------------------------------------------------------",
            "FUNCTION_BLOCK \"${1:Name}\"\nTITLE = '[V#01.01.01.01] ${2:Short description}'",
            "\/\/Copyright (c) 20xx Copyright ${3:Author} All Rights Reserved.",
            "",
            "",
            "",
            "\/\/===============================================================================",
            "",
            "",
            "",
            "",
            "\/\/[V#01.01.01.01],${3:Author},${4:date}\t\tOriginal Version",
            "AUTHOR\t :\t${3:Author}",
            "FAMILY\t :\t${5:Family}",
            "NAME\t\t :\t${1:Name}",
            "VERSION\t:\t${7:'1.0'}",
            "KNOW_HOW_PROTECT",
            "\/\/------------------------------------------------------------------------------------",
            "\/\/  Declaration of Variables",
            "\/\/------------------------------------------------------------------------------------",
            "${8:}",
            "CONST",
            "\t\/\/constants",
            "END_CONST",
            "",
            "VAR_INPUT",
            "\t\/\/inputs",
            "END_VAR",
            "",
            "VAR_IN_OUT",
            "\t\/\/in_outs",
            "END_VAR",
            "",
            "VAR_OUTPUT",
            "\t\/\/outputs",
            "END_VAR",
            "",
            "VAR",
            "\t\/\/variables",
            "END_VAR",
            "",
            "VAR_TEMP",
            "\t\/\/temporary variables",
            "END_VAR",
            "",
            "\/\/------------------------------------------------------------------------------------",
            "\/\/  Code Section",
            "\/\/------------------------------------------------------------------------------------",
            "",
            "BEGIN",
            "\/\/code",
            "END_FUNCTION_BLOCK"
        ]

    },

